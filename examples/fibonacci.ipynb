    "collapsed": false
   },
   "outputs": [],
   "source": [
    "from streamz import Stream\n",
    "source = Stream()\n",
    "\n",
    "s = source.sliding_window(2).map(sum)\n",
    "s.rate_limit(0.5).sink(source.emit)\n",
    "\n",
    "L = s.sink_to_list()\n",
    "s.sink(print)"
   ]
  },
